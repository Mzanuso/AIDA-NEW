{
  "name": "@aida/style-selector",
  "version": "1.0.0",
  "description": "AIDA Style Selector Service - Visual style management and selection",
  "main": "server.ts",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.json server.ts",
